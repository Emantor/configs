!!-- Xft settings -- !
! Transparency - true or false (default)
!URxvt*transparent: true
!URxvt*tintColor: Blue

! shading - 0 to 99 darkens, 101 to 200 lightens.
!   Don't use with tintColor; just use a darker or lighter color instead.
!URxvt*shading: 5

! scrollback buffer lines - 65535 is max on most machines (64 is default)
URxvt*saveLines: 32767

! font color (default is black)
!URxvt*foreground: White

! background color (prior to tinting) (default is white)
!URxvt*background: Black

URxvt*letterSpace: 0

URxvt*font: xft:Fantasque Sans Mono:style=Regular:size=10

! scrollbar - true (default) or false
URxvt*scrollBar: false

! scrollbar position - left=false (default) or right=true
URxvt*scrollBar_right: false

! scrollbar style - rxvt (default), plain, next, or xterm
URxvt*scrollstyle: rxvt

URxvt*termName: rxvt-256color

URxvt.depth: 32
URxvt*background: [75]#000000

#URxvt.fading: 50
#URxvt.fadeColor: [0]black
