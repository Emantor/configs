# Deaktiviere Terminal Beep
xset -b
# Setze Kopfhörer Lautstärke auf 50%
amixer set Headphone playback 32
# Starte Compositing Manager
xcompmgr -c &
# Trayer
trayer --edge top --align right --SetDockType true --SetPartialStrut true --expand true --width 6 --transparent true --alpha 0 --tint 0x000000 --height 16 &
# dunst
dunst &
# feh
feh --bg-scale /home/phoenix/Bilder/bg.png
